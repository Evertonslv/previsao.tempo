package model;

public class Cidade {

	private String nome;
    private int codigo;

    public Cidade() {
        super();
    }

    public Cidade(int codigoCidade, String nomeCidade) {
        this.nome = nomeCidade;
        this.codigo = codigoCidade;
    }

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public int getCodigo() {
		return codigo;
	}

	public void setCodigo(int codigo) {
		this.codigo = codigo;
	}
	
}
