package previsao.tempo.model;

public class city {

	private String name;
    private int codigo;

    public city() {
        super();
    }

    public city(int codeCity, String nameCity) {
        this.name = nameCity;
        this.codigo = codeCity;
    }

	public String getNome() {
		return name;
	}

	public void setNome(String nome) {
		this.name = nome;
	}

	public int getCodigo() {
		return codigo;
	}

	public void setCodigo(int codigo) {
		this.codigo = codigo;
	}
	
}
